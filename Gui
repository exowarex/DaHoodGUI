local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("prankster#0001", "Serpent")
    -- MAIN
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")


    MainSection:NewButton("Blue Dot", "no fps drops", function()
        loadstring(game:HttpGet("raw.githubusercontent.com/narv2poor/DaHoodGUI/main/BlueDot", true))()
    end)

    MainSection:NewButton("Camlock", "No Fps drops", function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/narv2poor/DaHoodGUI/main/Camlock'),true))()
    end)



    --AntiLock
    local Other = Window:NewTab("Antilock")
    local OtherSection = Other:NewSection("Other")

    OtherSection:NewButton("UnderGround (X)", "UnderGround antiaim", function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/narv2poor/DaHoodGUI/main/UnderGround'),true))()
    end)
    
       OtherSection:NewButton("AimViewer", "view aim", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Nosssa/NossLock/main/VinGUI"))()
    end)
